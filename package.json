{
  "name": "jhipster-sample-application",
  "version": "0.0.0",
  "private": true,
  "description": "JHipster Sample Application",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.17.1"
  },
  "jestSonar": {
    "reportPath": "target/test-results",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-path=target/classes/static",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --coverage",
    "lint": "ng lint",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\""
  },
  "devDependencies": {
    "@angular-eslint/builder": "16.1.1",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "16.1.1",
    "@angular-eslint/schematics": "16.1.1",
    "@angular-eslint/template-parser": "16.1.1",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.48.0",
    "@angular-builders/jest": "16.0.1",
    "@angular-devkit/build-angular": "16.2.0",
    "@angular/cli": "16.2.0",
    "@angular/compiler-cli": "16.2.2",
    "@types/node": "20.5.7",
    "@types/jest": "29.5.4",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "ts-jest": "29.1.1",
    "jest-preset-angular": "13.1.1",
    "jest-sonar-reporter": "2.0.0",
    "typescript": "5.1.6",
    "@prettier/plugin-xml": "3.2.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "prettier": "3.0.2",
    "prettier-plugin-java": "2.3.0",
    "prettier-plugin-packagejson": "2.4.5"
  },
  "dependencies": {
    "@angular/animations": "16.2.2",
    "@angular/cdk": "16.2.1",
    "@angular/common": "16.2.2",
    "@angular/compiler": "16.2.2",
    "@angular/core": "16.2.2",
    "@angular/material": "16.2.1",
    "@angular/forms": "16.2.2",
    "@angular/platform-browser": "16.2.2",
    "@angular/platform-browser-dynamic": "16.2.2",
    "@angular/router": "16.2.2",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.13.1"
  }
}
